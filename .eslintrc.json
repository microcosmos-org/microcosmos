{
    "env": {
        "browser": false,
        "node": true,
        "es2021": true
    },
    "extends": ["eslint:recommended", "google"],
    "parserOptions": {
        "ecmaVersion": 2021,
        "sourceType": "module"
    },
    "rules": {
        "require-jsdoc" : 0,
        "max-len": ["error", {"code": 120}],
        "arrow-parens": 0,
        "object-curly-spacing": ["error", "always"]
    }
}
